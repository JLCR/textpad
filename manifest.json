{
    "manifest_version" : 2,
    "name": "textpad",
    "version": "0.0.1",
    "description": "Write anything in a tab and save it",
    "browser_action": {
        "default_icon": "icons/icon_16x16.png"
    },
    "chrome_url_overrides" : {
        "newtab" : "index.html"
    },
    "icons" : {
      "16" : "icons/icon_16x16.png",
      "48" : "icons/icon_48x48.png",
      "128" : "icons/icon_128x128.png"
    }
}
